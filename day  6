####################### DAY 6 ###################################

########### SUM OF No. DIVISIBLE BY 3 #########################

#include<iostream>
using namespace std;
int main(){
    int n=10,i=1,sum=0;
    while(i<=n){
        if(i%3==0){
            sum+=i;
        }
        i++;

    }
    cout<<"sum="<<sum<<endl;
    

    return 0;
}

