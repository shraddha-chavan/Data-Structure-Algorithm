#####  SUM OF TWO No. USING FUNCTION ############################

#include<iostream>
using namespace std;

double sum(double a, double b){
        double s=a+b;
        return s;
}
int main(){
    cout<<sum(10.99,5.87)<<endl;
    


    return 0;
}

