################## VECTOR  ##########################

## CREATION OF VECTOR ##

#include<iostream>
#include<vector>
using namespace std;
//1)) vector <int> vec;
int main(){
    vector<int> vec;//0
    cout<<vec[0];
    return 0;
}
//2)) vector <int> vec={1,2,3}

vector<int> vec={1,2,3};//3
cout<<vec[0]<<endl;
return 0;

//3)) vector <int> vec(3,0)

vector<int> vec(5,0);
cout<<vec[0] <<endl;
return 0; 


